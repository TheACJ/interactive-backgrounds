{
  "name": "interactive-backgrounds",
  "version": "1.0.0",
  "description": "Interactive canvas-based background components for React",
  "type": "module",
  "main": "dist/index.umd.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.umd.js"
    }
  },
  "scripts": {
    "build": "tsc && webpack --mode production",
    "typecheck": "tsc --noEmit",
    "storybook": "node -r ./.storybook/shim.js node_modules/.bin/storybook dev -p 6006",
    "build:storybook": "storybook build"
  },
  "keywords": [
    "react",
    "canvas",
    "background",
    "interactive",
    "animation",
    "particles",
    "matrix",
    "aurora",
    "nebula",
    "quantum"
  ],
  "author": "Agbai C Joshua",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/TheACJ/interactive-backgrounds"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.27.1",
    "@storybook/addon-docs": "9.1.3",
    "@storybook/jest": "^0.2.2",
    "@storybook/react": "9.1.3",
    "@storybook/react-vite": "^9.1.3",
    "@storybook/testing-library": "^0.2.1",
    "@types/react": "^18.0.0",
    "@types/three": "^0.178.0",
    "babel-loader": "^10.0.0",
    "react": "^18.0.0",
    "react-dom": "18.0.0",
    "ts-loader": "^9.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.4.19",
    "webpack": "^5.0.0",
    "webpack-cli": "^4.0.0"
  },
  "dependencies": {
    "three": "^0.178.0"
  }
}